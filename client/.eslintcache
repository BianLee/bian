lementContainer\n                   email={email}\n                   setEmail={setEmail}\n                   password={password}\n                   setPassword={setPassword}\n                   username={username}\n                   setUsername={setUsername}\n                   handleLogin={handleLogin}\n                   handleSignup={handleSignup}\n                   hasAccount={hasAccount}\n                   setHasAccount={setHasAccount}\n                   emailError={emailError}\n                   passwordError={passwordError} \n                />\n\n            </>\n        )\n    }\n\n}  \n\n","/home/bian/Desktop/CS/Web/bian/client/src/firebaseApp.js",[],"/home/bian/Desktop/CS/Web/bian/client/src/containers/loginelement.js",["116"],"import React, {useState} from \"react\"\nimport { LoginElement } from \"../components/index\" \n// import \"../styles/loginElement.css\"\nimport \"../loginElement.css\" \nexport function LoginElementContainer(props) {\n    const {\n        email, \n        setEmail, \n        password,\n        setPassword,\n        username,\n        setUsername, \n        handleLogin, \n        handleSignup,\n        hasAccount, \n        setHasAccount,\n        emailError, \n        passwordError\n    } = props; \n\n    return (\n        <> \n            <LoginElement.Label>Email</LoginElement.Label>\n                    <input\n                        className=\"acc-input\"\n                        type=\"text\" \n                        autoFocus \n                        required \n                        value={email} \n                        onChange={(e) => setEmail(e.target.value)}\n                    />\n                    \n                <LoginElement.Label>Password</LoginElement.Label>\n                    <input\n                        className=\"acc-input\"\n                        type=\"password\"\n                        required\n                        value={password}\n                        onChange={(e) => setPassword(e.target.value)}\n                    />\n\n             {!hasAccount ? (\n                 <></>\n             ) : (\n                 <><LoginElement.Label>Organization Name</LoginElement.Label>\n                 <input\n                  className=\"acc-input\"\n                    type=\"text\" \n                    autoFocus \n                    required \n                    value={username}\n                    onChange={(e) => setUsername(e.target.value)}\n            />  </>\n\n             )}\n\n                    \n                <LoginElement.ButtonContainer width=\"100%\" >\n                    <div className=\"btnContainer\">\n                        {!hasAccount ? (\n                            <>\n                                <button className=\"authButton\" onClick={handleLogin}>Sign In</button>\n                                <p className=\"errorMsg\">{emailError}</p>\n                                <p className=\"errorMsg\">{passwordError}</p>\n                                <p className=\"signP\">\n                                <span className=\"signSpan\" onClick={() => { setHasAccount(!hasAccount);}}>Sign up</span></p>\n                            </>\n                        ) : (\n                            <>\n                                <button className=\"authButton\" onClick={handleSignup}>Sign Up</button>\n                                <p className=\"errorMsg\">{passwordError}</p>\n                                <p className=\"errorMsg\">{emailError}</p>\n                                <p className=\"signP\">\n                                <span className=\"signSpan\" onClick={() => { setHasAccount(!hasAccount);}}>Sign In</span></p>\n                            </>\n                        )}\n                    </div>\n                </LoginElement.ButtonContainer>\n                \n        </>\n    )\n}","/home/bian/Desktop/CS/Web/bian/client/src/containers/hero.js",["117"],"import React, {useEffect, u