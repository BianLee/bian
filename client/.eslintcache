         \n            } else {\n                setUser(\"\"); \n            }\n        })\n        \n    }\n\n    useEffect(() => {\n        authListener(); \n    }, []) \n    \n\n    if (user) {\n        return (\n            \n            <Hero username={currentUser} handleLogout={handleLogout}/>\n        )\n    }\n    else {\n        return (\n            <>\n                <LoginElementContainer\n                   email={email}\n                   setEmail={setEmail}\n                   password={password}\n                   setPassword={setPassword}\n                   username={username}\n                   setUsername={setUsername}\n                   handleLogin={handleLogin}\n                   handleSignup={handleSignup}\n                   hasAccount={hasAccount}\n                   setHasAccount={setHasAccount}\n                   emailError={emailError}\n                   passwordError={passwordError} \n                />\n\n            </>\n        )\n    }\n\n}  \n\n","/home/bian/Desktop/CS/Web/bian/client/src/firebaseApp.js",[],"/home/bian/Desktop/CS/Web/bian/client/src/containers/loginelement.js",["111"],"import React, {useState} from \"react\"\nimport { LoginElement } from \"../components/index\" \n// import \"../styles/loginElement.css\"\nimport \"../loginElement.css\" \nexport function LoginElementContainer(props) {\n    const {\n        email, \n        setEmail, \n        password,\n        setPassword,\n        username,\n        setUsername, \n        handleLogin, \n        handleSignup,\n        hasAccount, \n        setHasAccount,\n        emailError, \n        passwordError\n    } = props; \n\n    return (\n        <> \n            <LoginElement.Label>Email</LoginElement.Label>\n                    <input\n                        className=\"acc-input\"\n                        type=\"text\" \n                        autoFocus \n                        required \n                        value={email} \n                        onChang